@model AvtoMnenie.Models.NavigationListModel
@for (int i = 0; i < Model.ActionName.Count; i++)
{
    <span style="float:left">
        @Html.ActionLink(Model.LinkText[i], Model.ActionName[i], Model.ControllerName[i])
    </span>
    <span style="margin-left: 10px; margin-right: 10px; float:left">></span>
}
<span style="color: gray">@Model.TargetName</span>